essage() {\r\n        return this.message;\r\n    }\r\n\r\n    serialize() {\r\n        return {\r\n            user: this.user, message: this.message, timestamp: this.timestamp\r\n        };\r\n    }\r\n}\r\nexport default ChatApp;"},"lineCount":null}},"error":null,"hash":"24c76cec20894dfb501124ec3e3b60ae","cacheData":{"env":{}}}